{
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gmencz/faunadb"
  },
  "bugs": {
    "url": "https://github.com/gmencz/faunadb/issues"
  },
  "homepage": "https://github.com/gmencz/faunadb#readme",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "dotenv -- tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "np --access public"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gmencz/faunadb",
  "author": "gmencz",
  "module": "dist/faunadb.esm.js",
  "size-limit": [
    {
      "path": "dist/faunadb.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/faunadb.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "dotenv-cli": "^4.0.0",
    "husky": "^7.0.2",
    "np": "^7.5.0",
    "size-limit": "^5.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  }
}
