{
  "name": "@gmencz/faunadb",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "gmencz",
  "keywords": [
    "faunadb",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gmencz/faunadb"
  },
  "bugs": {
    "url": "https://github.com/gmencz/faunadb/issues"
  },
  "homepage": "https://github.com/gmencz/faunadb#readme",
  "scripts": {
    "build": "NODE_ENV=production tsup-node",
    "build:dev": "NODE_ENV=development tsup-node",
    "dev": "node-dev dist/main.js",
    "test": "dotenv -- jest",
    "format": "pretty-quick --staged",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release --branches main",
    "prepare": "husky install",
    "commit": "cz"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/esm/index.js",
      "limit": "10 KB"
    }
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@size-limit/preset-small-lib": "^5.0.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^4.0.0",
    "esbuild": "^0.13.2",
    "esbuild-jest": "^0.5.0",
    "husky": "^7.0.0",
    "jest": "^27.2.2",
    "node-dev": "^7.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "semantic-release": "^18.0.0",
    "size-limit": "^5.0.4",
    "tslib": "^2.3.1",
    "tsup": "^5.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "ts-toolbelt": "^9.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
